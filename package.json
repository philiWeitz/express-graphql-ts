{
  "name": "express-graphql-ts",
  "version": "0.1.0",
  "private": true,
  "description": "Express GraphQL boilerplate",
  "main": "src/server.js",
  "author": "Philipp Weitz",
  "license": "MIT",
  "engines": {
    "node": "^8.9.0"
  },
  "scripts": {
    "build": "yarn tsc",
    "watch-ts": "yarn tsc -- --watch",
    "watch-js": "nodemon --inspect=0.0.0.0:65505 ./build/server.js",
    "start-debug": "concurrently \"yarn watch-ts\" \"yarn watch-js\"",
    "migrate-db": "knex migrate:latest",
    "seed-db": "knex seed:run"
  },
  "dependencies": {
    "@types/express": "4.11.1",
    "@types/ioredis": "3.2.5",
    "@types/node": "9.4.6",
    "@types/redis": "2.8.5",
    "apollo-server-express": "1.3.2",
    "body-parser": "1.18.2",
    "concurrently": "3.5.1",
    "cors": "2.8.4",
    "express": "4.16.2",
    "express-jwt": "5.3.0",
    "graphql": "0.12.3",
    "graphql-depth-limit": "1.1.0",
    "graphql-directive": "0.2.1",
    "graphql-redis-subscriptions": "1.4.0",
    "graphql-server-express": "1.3.2",
    "graphql-subscriptions": "0.5.8",
    "graphql-tools": "2.21.0",
    "jsonwebtoken": "8.1.1",
    "knex": "0.14.2",
    "lodash": "4.17.5",
    "objection": "1.0.0",
    "pg": "7.4.0",
    "redis": "2.8.0",
    "subscriptions-transport-ws": "0.9.6",
    "typescript": "2.7.2"
  },
  "devDependencies": {
    "nodemon": "1.15.1"
  }
}
